.container {
    padding-bottom: calc(55px + env(safe-area-inset-bottom))
}

.user-info {
    background-color: #343638;
    background-image: url("data:img/jpg;base64,");
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: cover;
    border-radius: 16rpx;
    box-shadow: 0 5px 20px 0 rgba(0,0,0,.34);
    min-height: 160rpx
}

.status-bar {
    display: -webkit-flex;
    display: flex;
    line-height: 60rpx;
    padding: 30rpx 43rpx 26rpx
}

.status-bar .photo {
    border: 2rpx solid #fff;
    border-radius: 50%;
    height: 60rpx;
    margin-top: -4rpx;
    min-width: 60rpx;
    width: 60rpx
}

.status-bar .username {
    color: #fff;
    -webkit-flex: 1;
    flex: 1;
    font-size: 28rpx;
    margin-left: 14rpx
}

.status-bar .fun-icon {
    color: #fff;
    font-size: 24rpx;
    line-height: 60rpx
}

.message-wrap {
    -webkit-align-items: center;
    align-items: center;
    background: #f4f1e7;
    display: -webkit-flex;
    display: flex;
    height: 98rpx;
    padding: 0 30rpx;
    position: relative
}

.message-wrap > .fun-icon-angle-right {
    color: #333;
    font-size: 20rpx;
    margin-right: 17rpx
}

.warm-tip {
    -webkit-align-items: center;
    align-items: center;
    border-top: 1px dashed hsla(0,0%,100%,.31);
    color: #ffe28a;
    display: -webkit-flex;
    display: flex;
    padding: 20rpx 30rpx
}

.mseeage-icon {
    height: 32rpx;
    margin-left: 13rpx;
    width: 34rpx
}

.has-message:after {
    background: #ff5c5e;
    border-radius: 50%;
    content: "";
    height: 10rpx;
    left: 70rpx;
    position: absolute;
    top: 28rpx;
    width: 10rpx
}

.message-list {
    color: #ffe28a;
    -webkit-flex: 1;
    flex: 1;
    font-size: 24rpx;
    height: 70rpx;
    margin-left: 16rpx;
    margin-right: 20rpx
}

.message-item {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    height: 100%;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    padding-right: 20rpx
}

.message-content {
    word-wrap: break-word;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    color: #212721;
    display: -webkit-box;
    -webkit-flex: 1;
    flex: 1;
    font-size: 28rpx;
    font-weight: 500;
    overflow: hidden;
    text-overflow: ellipsis
}

.service-head {
    width: 100%
}

.service-head-bg {
    height: 422rpx;
    width: 100%
}

.service-head-bg wx-image {
    height: 100%;
    width: 100%
}

.service-head-content {
    color: #693b29;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    font-size: 24rpx;
    font-weight: 500;
    left: 0;
    margin-left: 80rpx;
    margin-top: 30rpx;
    position: absolute;
    right: 0;
    top: 98rpx
}

.service-head-name-container {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
    flex-direction: row;
    height: 50rpx;
    line-height: 50rpx
}

.service-head-name {
    font-size: 36rpx
}

.service-head-name-edit {
    height: 26rpx;
    margin-left: 10rpx;
    width: 26rpx
}

.service-welcome-slogan {
    margin-top: 5rpx
}

.live-period-wrapper {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.live-period-text {
    margin-left: 10rpx
}

.activity-info {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    height: 44rpx;
    -webkit-justify-content: center;
    justify-content: center;
    margin-top: 90rpx;
    width: 260rpx
}

.activity-info-title {
    color: #fff
}

.service-head-renew-arrow {
    height: 18rpx;
    margin-left: 10rpx;
    width: 11rpx
}

.service-common-feature {
    color: #333;
    font-size: 38rpx;
    font-weight: 500;
    margin: 40rpx 40rpx 0
}

.service-slogan {
    color: #777;
    font-size: 24rpx;
    font-weight: 400;
    margin: 5rpx 40rpx 10rpx
}

.card-list {
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap
}

.card,.card-list {
    display: -webkit-flex;
    display: flex
}

.card {
    -webkit-align-items: center;
    align-items: center;
    -webkit-flex-direction: column;
    flex-direction: column;
    padding: 30rpx 0;
    position: relative;
    text-align: center;
    width: 25%
}

.card-icon {
    height: 60rpx;
    width: 60rpx
}

.card-name {
    font-size: 24rpx;
    margin-top: 10rpx
}

.card-brand {
    background: linear-gradient(180deg,#fc777d,#ff5e62);
    border-radius: 16rpx;
    color: #fff;
    font-size: 20rpx;
    height: 32rpx;
    line-height: 32rpx;
    min-width: 32rpx;
    padding-left: 6rpx;
    padding-right: 6rpx;
    position: absolute;
    right: 18%;
    top: 18%
}

.user-info {
    position: relative;
    z-index: 1
}

.user-info-bg {
    border-radius: 16rpx;
    box-shadow: 0 5px 20px 0 rgba(0,0,0,.34);
    width: 100%
}

.user-info-main-view {
    bottom: 0rpx;
    height: auto;
    left: 0rpx;
    padding: 47rpx 43rpx 0rpx;
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    width: auto;
    z-index: 1
}

.login-view {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.login-view-image {
    border-radius: 30rpx;
    height: 60rpx;
    min-width: 60rpx;
    width: 60rpx
}

.login-view-text {
    color: #fff;
    -webkit-flex-shrink: 1;
    flex-shrink: 1;
    font-family: PingFangSC;
    font-size: 28rpx;
    font-weight: 500;
    margin-left: 14rpx;
    overflow: hidden;
    text-overflow: ellipsis
}

.bottom-ad-image-view {
    line-height: 0;
    margin-top: 20px
}

.bottom-ad-image {
    height: 100%;
    width: 100%
}

.floating-window {
    bottom: 100rpx;
    height: 120rpx;
    position: fixed;
    right: 0;
    width: 120rpx
}

.floating-window-content {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    height: 100%;
    -webkit-justify-content: center;
    justify-content: center;
    width: 100%
}

.floating-window-content-img {
    height: 120rpx;
    width: 95rpx
}
