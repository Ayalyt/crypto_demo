page {
    background: #f7f7f7
}

.visitor-access-register {
    height: 100vh;
    overflow-y: scroll;
    padding-bottom: 200rpx
}

.visitor-access-register .title {
    color: #333;
    font-size: 38rpx;
    font-weight: 500;
    line-height: 70rpx;
    padding-left: 40rpx;
    padding-top: 10rpx
}

.visitor-access-register .basic-info {
    background-color: #fff;
    margin-bottom: 20rpx
}

.visitor-access-register .basic-info-readonly {
    background-color: #fff;
    margin-bottom: 20rpx;
    padding-bottom: 20rpx
}

.visitor-access-register .basic-info-readonly .title {
    margin-bottom: 40rpx
}

.visitor-access-register .basic-info-readonly .item {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    padding: 0 40rpx 30rpx
}

.visitor-access-register .basic-info-readonly .item .label {
    color: #949aa7;
    font-size: 28rpx;
    font-weight: 400;
    height: 40rpx;
    line-height: 40rpx;
    padding-right: 50rpx;
    width: 115rpx
}

.visitor-access-register .basic-info-readonly .item .value {
    color: #333;
    font-size: 28rpx;
    font-weight: 400;
    height: 40rpx;
    line-height: 40rpx
}

.visitor-access-register .access-info .item .label {
    font-size: 30rpx;
    font-weight: 400;
    padding-right: 50rpx
}

.visitor-access-register .access-info {
    background-color: #fff;
    margin-bottom: 20rpx
}

.visitor-access-register .access-info .item {
    -webkit-align-items: center;
    align-items: center;
    border-bottom: 2rpx solid #efefef;
    display: -webkit-flex;
    display: flex;
    margin-left: 40rpx;
    margin-right: 40rpx;
    padding: 20px 0
}

.visitor-access-register .access-info .item .info-value {
    font-size: 30rpx;
    font-weight: 500
}

.visitor-access-register .access-info .item .tab-value {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
    margin-top: 10rpx
}

.visitor-access-register .access-info .item .tab-value .faq-tab {
    background-color: #f6f6f6;
    border-radius: 28rpx;
    font-size: 24rpx;
    line-height: 56rpx;
    margin-right: 20rpx;
    text-align: center;
    width: 150rpx
}

.visitor-access-register .access-info .item .tab-value .faq-tab.active {
    background: #212721;
    color: #ffda71
}

.visitor-access-register .tip {
    color: #828999;
    font-size: 24rpx;
    font-weight: 400;
    height: 33rpx;
    line-height: 33rpx;
    padding: 20rpx 40rpx;
    text-align: center
}

.visitor-access-register .footer-btn {
    background-color: #fff;
    bottom: 0;
    box-sizing: border-box;
    padding: 30rpx 40rpx 60rpx;
    position: fixed;
    width: 100%
}
