.visitor-access-lock-list {
    overflow-x: hidden
}

.visitor-access-lock-list .header {
    background: #212721;
    color: #fff;
    font-size: 24rpx;
    font-weight: 400;
    height: 88rpx;
    line-height: 88rpx;
    padding: 0 40rpx;
    width: 100%
}

.visitor-access-lock-list .header .count-down {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.visitor-access-lock-list .header .count-down .count-down-text {
    color: #fff;
    font-size: 35rpx;
    font-weight: 500;
    line-height: 88rpx;
    padding-left: 20rpx
}

.visitor-access-lock-list .lock-list {
    padding: 50rpx 40rpx
}

.visitor-access-lock-list .lock-list .empty {
    width: 100%
}

.visitor-access-lock-list .lock-list .title {
    color: #333;
    font-size: 38rpx;
    font-weight: 500;
    height: 48rpx;
    line-height: 48rpx
}

.visitor-access-lock-list .lock-list-content {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    margin-bottom: 200rpx;
    padding-top: 40rpx
}

.visitor-access-lock-list .lock-list-content .lock-item {
    -webkit-align-items: center;
    align-items: center;
    background-color: #f4f4f4;
    border-radius: 10rpx;
    display: -webkit-flex;
    display: flex;
    height: 100rpx;
    margin-bottom: 30rpx;
    padding: 20rpx;
    width: 640rpx
}

.visitor-access-lock-list .lock-list-content .lock-item .name {
    color: #333;
    font-size: 34rpx;
    font-weight: 500;
    height: 33rpx;
    line-height: 33rpx;
    overflow: hidden;
    padding-left: 10rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 100%
}

.visitor-access-lock-list .lock-list-content .lock-item .btn {
    -webkit-align-items: center;
    align-items: center;
    background-color: #d8d8d8;
    border: none;
    border-radius: 40rpx;
    display: -webkit-flex;
    display: flex;
    height: 60rpx;
    -webkit-justify-content: center;
    justify-content: center;
    overflow: hidden;
    width: 125rpx
}

.visitor-access-lock-list .lock-list-content .lock-item .item-loading {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    min-width: 120rpx
}

.visitor-access-lock-list .lock-list-content .lock-item .loading-text {
    font-size: smaller
}

.visitor-access-lock-list .lock-list-content .lock-item .load_circle {
    -webkit-animation: circle-ani 1s linear infinite;
    animation: circle-ani 1s linear infinite;
    border: 6rpx solid rgba(0,0,0,.25);
    border-left-color: #333;
    border-radius: 50%;
    height: 50rpx;
    margin: 10px auto;
    position: relative;
    width: 50rpx
}

@-webkit-keyframes circle-ani {
    0% {
        -webkit-transform: rotate(0);
        transform: rotate(0)
    }

    100% {
        -webkit-transform: rotate(359deg);
        transform: rotate(359deg)
    }
}

@keyframes circle-ani {
    0% {
        -webkit-transform: rotate(0);
        transform: rotate(0)
    }

    100% {
        -webkit-transform: rotate(359deg);
        transform: rotate(359deg)
    }
}

.visitor-access-lock-list .lock-list-content .lock-item .btn .btn-text {
    color: #333;
    font-size: 28rpx;
    white-space: nowrap
}

.visitor-access-lock-list .lock-list-content .lock-item .power-img {
    float: right;
    height: 80rpx;
    width: 80rpx
}

.visitor-access-lock-list .footer-btn-group {
    -webkit-align-items: center;
    align-items: center;
    background-color: #fff;
    bottom: 0;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-around;
    justify-content: space-around;
    padding-bottom: 60rpx;
    padding-top: 30rpx;
    position: fixed;
    width: 100%
}

.visitor-access-lock-list .footer-btn {
    -webkit-flex: 1;
    flex: 1;
    margin: 0 40rpx
}
