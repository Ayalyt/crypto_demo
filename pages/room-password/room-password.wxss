.room-password-page {
    background-color: #f4f4f4;
    height: 100vh;
    overflow: hidden;
    width: 100%
}

.room-password-header {
    background: linear-gradient(270deg,#333,#2c2825);
    box-sizing: border-box;
    color: #fff;
    font-size: 24rpx;
    font-weight: 700;
    height: 88rpx;
    line-height: 88rpx;
    margin-bottom: 180rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
    width: 100%
}

.room-password-header .apartment-list-wraper {
    background: rgba(250,194,0,.44);
    border-radius: 30rpx;
    display: inline-block;
    height: 50rpx;
    line-height: 50rpx;
    padding: 0 36rpx;
    position: relative
}

.list-wraper-label {
    color: #fff;
    font-size: 24rpx;
    font-weight: 700;
    text-shadow: 0 2px 3px #e5b719
}

.list-wraper-label .down {
    border-color: #fff transparent transparent;
    border-style: solid;
    border-width: 14rpx 12rpx;
    height: 0;
    margin-right: 20rpx;
    position: absolute;
    right: 0rpx;
    top: 20rpx;
    width: 0
}

.apartment-list-wraper .list-wraper-label wx-text {
    margin-right: 15rpx
}

.room-password-content {
    background: #fff;
    box-sizing: border-box;
    height: calc(100vh - 420rpx)
}

.room-password-content .get-password-btn {
    background-color: #ffdf83;
    border-radius: 44px;
    box-shadow: 0 4px 21px 0 rgba(201,156,0,.37);
    color: #795600;
    font-size: 28rpx;
    height: 88rpx;
    line-height: 88rpx;
    margin-bottom: 40rpx;
    margin-top: 50rpx;
    text-align: center;
    width: 670rpx
}

.get-password-btn .fun-button {
    font-size: 28rpx;
    line-height: 88rpx
}

.get-password-text {
    color: #949aa7;
    font-size: 28rpx;
    margin-bottom: 20rpx
}

.room-password-box {
    padding: 30rpx;
    text-align: left
}

.room-password-label {
    color: #949aa7;
    font-size: 28rpx;
    font-weight: 400
}

.room-password-label-title {
    -webkit-align-items: center;
    align-items: center;
    border-bottom: 1rpx solid hsla(0,0%,96%,.8);
    color: #949aa7;
    display: -webkit-flex;
    display: flex;
    font-size: 24rpx;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    padding: 27rpx 0
}

.room-password-label-title-name {
    color: #333;
    font-size: 32rpx;
    font-weight: 500
}

.room-password-input {
    border: 1rpx solid #c8c8c8;
    border-radius: 4rpx;
    box-sizing: border-box;
    color: #333;
    display: -webkit-flex;
    display: flex;
    font-size: 60rpx;
    -webkit-justify-content: space-around;
    justify-content: space-around;
    margin-bottom: 26rpx;
    margin-top: 30rpx;
    padding-bottom: 20rpx;
    padding-top: 20rpx;
    width: 670rpx
}

.room-password-bottom {
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between
}

.room-password-refresh {
    color: #418df0;
    float: right;
    font-size: 26rpx
}

.room-password-item-refresh {
    color: #418df0;
    font-size: 26rpx;
    margin-right: 20rpx
}

.room-password-delete {
    color: #f15d61;
    float: right;
    font-size: 26rpx
}

.room-password-item-button {
    display: -webkit-flex;
    display: flex;
    margin-left: 270rpx;
    margin-top: 20rpx
}

.room-password-warn {
    margin-left: -200rpx
}

.room-password-icon {
    margin-bottom: 16rpx
}

.refresh-icon {
    float: right
}

.refresh-icon,.refresh-icon-sub {
    height: 40rpx;
    margin-right: 10rpx;
    width: 40rpx
}

.sub-item-button-refresh {
    display: -webkit-flex;
    display: flex;
    left: 62%;
    position: absolute
}

.sub-item-warn {
    margin-top: 10rpx
}

.sub-item-button-delete {
    left: 80%;
    position: absolute
}

.password-danger .room-password-input {
    background-color: #f5f5f5
}

.room-password-status {
    font-size: 24rpx
}

.password-danger .room-password-status {
    color: #f15d61
}

.password-success .room-password-status {
    color: #00d474
}

.room-password-date {
    color: #949aa7;
    font-size: 26rpx
}

.room-password-input-text {
    color: #333;
    font-size: 28rpx;
    letter-spacing: 6rpx
}

.room-password-error {
    text-align: center
}

.room-password-error wx-image {
    height: 240rpx;
    width: 298rpx
}

.room-password-error wx-view {
    color: #b8b8b8
}

.room-password-error wx-text {
    color: #0594ff
}

.room-password-device-wrapper {
    background: #fff;
    padding: 30rpx
}

.room-password-device {
    -webkit-align-items: center;
    align-items: center;
    border-bottom: 1rpx solid hsla(0,0%,96%,.8);
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    padding: 27rpx 0
}

.room-password-device-name {
    color: #333;
    font-size: 32rpx;
    font-weight: 500
}

.room-password-device-right {
    color: #949aa7;
    display: -webkit-flex;
    display: flex;
    font-size: 24rpx;
    font-weight: 400
}

.room-password-device-power {
    color: #f15d61
}

.room-password-fingerprint-tip {
    border-top: 1px solid #f4f4f4;
    color: #949aa7;
    font-size: 24rpx;
    font-weight: 400;
    margin-top: 27rpx;
    padding-top: 30rpx;
    text-align: center
}

.room-password-item {
    margin-bottom: 32rpx;
    margin-top: 20rpx
}

.room-password-item-name {
    color: #333;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column;
    font-size: 26rpx;
    font-weight: 400
}

.room-password-item-finger {
    margin-left: 10rpx;
    margin-top: 10rpx
}

.room-password-item-status {
    font-size: 24rpx;
    font-weight: 400;
    margin-left: 10rpx
}

.room-password-item-img-wrapper {
    display: -webkit-flex;
    display: flex;
    margin-bottom: 20rpx;
    margin-top: 30rpx
}

.room-password-item-img {
    background: #fff;
    border: 1rpx solid hsla(0,0%,78%,.4);
    border-radius: 4rpx;
    height: 70rpx;
    padding: 10rpx;
    width: 70rpx
}

.room-password-item-close {
    height: 40rpx;
    position: absolute;
    right: 265rpx;
    top: -20rpx;
    width: 40rpx
}

.room-password-item-bottom {
    color: #949aa7;
    display: -webkit-flex;
    display: flex;
    font-size: 24rpx;
    font-weight: 400;
    -webkit-justify-content: space-between;
    justify-content: space-between
}

.room-password-fiexd-bottom {
    background: #fff;
    bottom: 0;
    left: 0;
    padding: 50rpx 40rpx;
    position: fixed;
    right: 0
}

.password-danger.room-password-item-status {
    color: #f5f5f5;
    color: #f15d61
}

.password-success.room-password-item-status {
    color: #00d474
}

.room-password-bottom-refresh {
    height: 60rpx
}

.room-password-item-divider {
    background: #f4f4f4;
    height: 15rpx;
    margin: 0rpx -30rpx;
    width: 750rpx
}

.popup-img {
    height: 376rpx;
    width: 285rpx
}

.popup-count-down {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: center;
    justify-content: center
}
