.popup-mask {
    background: rgba(0,0,0,.5);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 1000
}

.popup-page {
    background: #fff;
    border-radius: 14rpx;
    box-sizing: border-box;
    left: 50%;
    min-height: 520rpx;
    padding: 30rpx 30rpx 20rpx;
    position: fixed;
    text-align: center;
    top: 47%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    width: 540rpx;
    z-index: 1001
}

.popup-close {
    background-image: url("data:image/png;base64,");
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: contain;
    border-radius: 50%;
    bottom: -120rpx;
    display: inline-block;
    height: 66rpx;
    position: absolute;
    -webkit-transform: inherit;
    transform: inherit;
    width: 66rpx
}

.popup-wrap {
    -webkit-overflow-scrolling: touch;
    max-height: 70vh;
    overflow: auto;
    overscroll-behavior-y: contain;
    text-align: center
}

.popup-title {
    color: #f01464;
    font-size: 46rpx;
    font-weight: 700;
    margin-bottom: 35rpx
}

.popup-title wx-image {
    height: 43rpx;
    max-width: 500rpx;
    vertical-align: top
}

.popup-content {
    margin-bottom: 30rpx
}

.fun-pop-top {
    -webkit-animation: fun-animate-pop-top .3s ease forwards;
    animation: fun-animate-pop-top .3s ease forwards
}

.fun-pop-mask {
    -webkit-animation: fun-animate-fadein .3s ease forwards;
    animation: fun-animate-fadein .3s ease forwards
}

@-webkit-keyframes fun-animate-pop-top {
    from {
        -webkit-transform: translate3d(10%,-300%,0);
        transform: translate3d(10%,-300%,0)
    }

    to {
        -webkit-transform: translate3d(10%,-120%,0);
        transform: translate3d(10%,-120%,0)
    }
}

@keyframes fun-animate-pop-top {
    from {
        -webkit-transform: translate3d(10%,-300%,0);
        transform: translate3d(10%,-300%,0)
    }

    to {
        -webkit-transform: translate3d(10%,-120%,0);
        transform: translate3d(10%,-120%,0)
    }
}