.fingerprint-success-mask {
    background: rgba(0,0,0,.5);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 1000
}

.fingerprint-success-page {
    background: #fff;
    border-radius: 6rpx;
    box-sizing: border-box;
    left: 50%;
    padding-top: 70rpx;
    position: fixed;
    text-align: center;
    top: 47%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    width: 540rpx;
    z-index: 1001
}

.fingerprint-success-title {
    -webkit-align-items: center;
    align-items: center;
    color: #333;
    display: -webkit-flex;
    display: flex;
    font-size: 50rpx;
    font-weight: 500;
    -webkit-justify-content: center;
    justify-content: center;
    letter-spacing: 1rpx;
    line-height: 70rpx;
    margin-bottom: 30rpx
}

.fingerprint-success-title-icon {
    height: 60rpx;
    margin-right: 20rpx;
    width: 60rpx
}

.fingerprint-success-message {
    color: #949aa7;
    font-size: 26rpx;
    font-weight: 400;
    margin-bottom: 30rpx;
    margin-top: 20rpx
}

.fingerprint-success-btn {
    -webkit-align-items: center;
    align-items: center;
    border-top: 1rpx solid #eaeaea;
    color: #949aa7;
    display: -webkit-flex;
    display: flex;
    font-size: 30rpx;
    font-weight: 400;
    height: 90rpx;
    -webkit-justify-content: center;
    justify-content: center;
    line-height: 42rpx;
    margin-top: 40rpx
}

.fingerprint-success-btn-item {
    height: 90rpx;
    line-height: 90rpx;
    width: 270rpx
}

.fingerprint-success-btn-item-retry {
    color: #e0ae01
}

.fingerprint-success-divider {
    background: #eaeaea;
    height: 90rpx;
    width: 1rpx
}

.fun-pop-mask {
    -webkit-animation: fun-animate-fadein .3s ease forwards;
    animation: fun-animate-fadein .3s ease forwards
}

@-webkit-keyframes fun-animate-pop-top {
    from {
        -webkit-transform: translate3d(10%,-300%,0);
        transform: translate3d(10%,-300%,0)
    }

    to {
        -webkit-transform: translate3d(10%,-120%,0);
        transform: translate3d(10%,-120%,0)
    }
}

@keyframes fun-animate-pop-top {
    from {
        -webkit-transform: translate3d(10%,-300%,0);
        transform: translate3d(10%,-300%,0)
    }

    to {
        -webkit-transform: translate3d(10%,-120%,0);
        transform: translate3d(10%,-120%,0)
    }
}