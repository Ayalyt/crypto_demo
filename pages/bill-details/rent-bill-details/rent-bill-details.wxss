page {
    background: #f4f4f4;
    height: 100%
}

.header {
    position: relative
}

.header-bg {
    width: 100%
}

.header-content {
    left: 0;
    padding: 49rpx 14px 0;
    position: absolute;
    right: 0;
    top: 0
}

.first-row {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between
}

.title {
    font-size: 32rpx
}

.status,.title {
    color: #fff;
    font-weight: 500
}

.status {
    font-size: 30rpx
}

.content {
    border-radius: 12rpx;
    left: 14px;
    padding-bottom: 150rpx;
    position: absolute;
    right: 14px;
    top: 168rpx;
    z-index: 2
}

.content-top-bg {
    width: 100%
}

.content-top-content {
    display: -webkit-flex;
    display: flex;
    left: 0;
    padding: 40rpx 14px 0;
    position: absolute;
    right: 0;
    top: 0
}

.content-top-image {
    background: #f4f4f4;
    border-radius: 12rpx;
    height: 150rpx;
    min-width: 167rpx;
    overflow: hidden;
    width: 167rpx
}

.room-info {
    color: #333;
    font-size: 28rpx
}

.rent-date {
    color: #949aa7;
    font-size: 24rpx;
    margin-top: 15rpx
}

.apartment-info {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    margin-top: 29rpx
}

.apartment-name {
    color: #949aa7;
    -webkit-flex-grow: 1;
    flex-grow: 1;
    font-size: 24rpx;
    margin-right: 10rpx
}

.apartment-line {
    background: #afb2bb;
    height: 24rpx;
    width: 1px
}

.phone {
    height: 30rpx;
    width: 140rpx
}

.content-bottom {
    background: #fff;
    border-radius: 0 0 12rpx 12rpx;
    padding: 0 14px
}

.content-bottom-subview {
    padding: 50rpx 0
}

.content-bottom-subview+.content-bottom-subview {
    border-top: .5px solid #eaeaea
}

.prepay-amount-view {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.content-title {
    color: #949aa7;
    font-size: 28rpx
}

.subtitle {
    color: #949aa7;
    font-size: 26rpx
}

.content-text {
    color: #333;
    font-size: 28rpx
}

.prepay-amount-title {
    color: #949aa7;
    font-size: 28rpx;
    width: 130rpx
}

.money {
    font-size: 26rpx;
    margin-left: 40rpx
}

.money,.prepay-amount {
    color: #e0ae01;
    font-weight: 500
}

.prepay-amount {
    font-size: 42rpx
}

.paymentInfo {
    padding: 0 0 50rpx
}

.line {
    background: #efefef;
    height: 1px
}

.paymentInfo-item {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.paymentInfo-item + .paymentInfo-item {
    margin-top: 10rpx
}

.paymentInfo-title {
    color: #949aa7;
    font-size: 24rpx;
    width: 100rpx
}

.paymentInfo-content {
    color: #333;
    font-size: 24rpx;
    margin-left: 65rpx
}

.remark {
    color: #949aa7;
    font-size: 26rpx;
    line-height: 50rpx
}

.fee-view {
    color: #333;
    font-size: 32rpx;
    font-weight: 500
}

.icon-arrow-down {
    height: 22rpx;
    width: 22rpx
}

.details {
    height: 40rpx;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    width: 85rpx
}

.details,.fee-view-item {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.fee-view-item {
    margin-top: 30rpx
}

.billInfos-view {
    background: #f4f4f4;
    box-shadow: inset 0 0 10px 0 rgba(0,0,0,.08);
    margin: 20rpx -14px 0
}

.billInfos {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-around;
    justify-content: space-around;
    padding: 14rpx 0
}

.statusDesc {
    color: #949aa7;
    font-size: 22rpx;
    margin-left: 3px
}

.copy-text {
    color: #e0ae01;
    font-size: 24rpx;
    margin-left: 4px
}

.copy-image {
    height: 24rpx;
    width: 24rpx
}

.tag-view {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    margin-top: 9rpx
}

.tag-item {
    border: 1px solid #949aa7;
    border-radius: 27rpx;
    color: #333;
    font-size: 24rpx;
    height: 54rpx;
    line-height: 54rpx;
    margin-top: 20rpx;
    text-align: center;
    width: 22%
}

.overdueFee-view {
    background: #fff;
    color: #949aa7;
    font-size: 24rpx;
    height: 156px;
    padding: 14px
}

.overdueFee-title {
    color: #333;
    font-size: 28rpx;
    font-weight: 700
}

.fun-button-left {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-grow: 1;
    flex-grow: 1;
    height: 78rpx;
    line-height: 78rpx;
    margin-left: 20rpx;
    text-align: center
}

.prepay {
    color: #333;
    font-size: 28rpx;
    font-weight: 400
}

.charge-amount {
    font-size: 28rpx
}

.charge-amount,.money-bottom {
    color: #e0ae01;
    font-weight: 500
}

.money-bottom {
    font-size: 20rpx;
    margin-left: 15rpx
}
