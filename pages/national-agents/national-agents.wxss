.wrapper {
    padding: 0 42rpx
}

.flex {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex
}

.height102 {
    height: 102rpx;
    position: relative
}

.height102::after {
    background-color: #f5f5f5;
    bottom: 0;
    content: "";
    height: 1px;
    left: 0;
    position: absolute;
    right: 0
}

.full-width {
    width: 100%
}

.flex-between {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between
}

.color-some-grey {
    color: #afb2bb
}

.size20 {
    font-size: 26rpx;
    line-height: 26rpx
}

.normal-lineheight {
    line-height: normal
}

.ml20 {
    margin-left: 20rpx
}

.ml30 {
    margin-left: 30rpx
}

.mr30 {
    margin-right: 30rpx
}

.width160 {
    height: 102rpx;
    line-height: 102rpx;
    width: 160rpx
}

.required::before {
    color: red;
    content: "*";
    left: -10rpx;
    position: relative
}

wx-radio .wx-radio-input {
    border-radius: 50%;
    height: 32rpx;
    line-height: 32rpx;
    margin-top: -3rpx;
    width: 32rpx
}

wx-radio .wx-radio-input.wx-radio-input-checked::before {
    background: transparent;
    border-radius: 50%;
    color: #fff;
    font-size: 32rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    transform: translate(-50%,-50%) scale(1);
    -webkit-transform: translate(-50%,-50%) scale(1);
    width: 32rpx
}

.fun-text-input-value {
    color: #828999;
    -webkit-flex: 1;
    flex: 1;
    height: 102rpx;
    line-height: 102rpx;
    text-overflow: ellipsis;
    width: 100%
}
